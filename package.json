{
    "name": "graphql-deploy",
    "version": "1.0.0",
    "description": "Boilerplate for React / GraphQL apps",
    "main": "index.js",
    "author": "Julien Sebag",
    "license": "MIT",
    "private": false,
    "dependencies": {
        "concurrently": "^8.0.1"
    },
    "scripts": {
        "install": "cd server && yarn && cd ../client && yarn",
        "server": "cd server && yarn dev",
        "client": "cd client && yarn dev",
        "dev": "concurrently --kill-others 'yarn server' 'yarn client'"
    }
}
